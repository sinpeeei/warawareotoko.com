}</style><script>document.addEventListener('DOMContentLoaded', function() {
    setTimeout(function() {
        const expanderWrappers = document.querySelectorAll('.my-expander-wrapper');
        const limitType = 'words';
        const limitValue = 120;
        function getPageLanguage() {
            const htmlLang = document.documentElement.lang;
            return htmlLang ? htmlLang.split('-')[0] : 'pl'; 
        }
        const currentLanguage = getPageLanguage();
        const buttonTexts = {
            'pl': {
                readMore: 'Więcej',
                readLess: 'Mniej'
            },
            'en': {
                readMore: 'More',
                readLess: 'Less'
            }
        };

        expanderWrappers.forEach(wrapper => {
            const intermediateDiv = wrapper.querySelector('.uk-card.uk-card-default');
            const contentDiv = wrapper.querySelector('.my-expander-content');
            
            let toggleButton = document.createElement('button');
            toggleButton.className = 'my-expander-toggle';
            toggleButton.textContent = buttonTexts[currentLanguage].readMore;

            if (!intermediateDiv || !contentDiv) {
                return;
            }

            const fullContentHTML = contentDiv.innerHTML;
            function truncateContentWithHtml(htmlString, type, limit) {
                const tempDiv = document.createElement('div');
                tempDiv.innerHTML = htmlString;
                
                let currentCount = { value: 0 };
                let needsEllipsis = false;
                const resultNodes = [];

                function processNode(node) {
                    if (node.nodeType === Node.TEXT_NODE) {
                        let textContent = node.textContent;
                        let count = (type === 'words') ? textContent.split(/\s+/).filter(w => w.length > 0).length : textContent.length;

                        if (currentCount.value + count >= limit) {
                            let remainingLimit = limit - currentCount.value;
                            let truncatedText;

                            if (type === 'words') {
                                const words = textContent.split(/\s+/).filter(w => w.length > 0);
                                truncatedText = words.slice(0, remainingLimit).join(' ');
                                if (words.length > remainingLimit) needsEllipsis = true;
                            } else {
                                truncatedText = textContent.substring(0, remainingLimit);
                                if (textContent.length > remainingLimit) needsEllipsis = true;
                            }
                            resultNodes.push(document.createTextNode(truncatedText));
                            currentCount.value = limit;
                            return true;
                        } else {
                            currentCount.value += count;
                            resultNodes.push(node.cloneNode(true));
                        }
                    } else if (node.nodeType === Node.ELEMENT_NODE) {
                        let newElement = node.cloneNode(false);
                        for (let i = 0; i < node.childNodes.length; i++) {
                            if (processNode(node.childNodes[i])) {
                                newElement.appendChild(resultNodes.pop());
                                resultNodes.push(newElement);
                                return true;
                            }
                        }
                        resultNodes.push(newElement);
                    } else {
                        resultNodes.push(node.cloneNode(true));
                    }
                    return false;
                }

                for (let i = 0; i < tempDiv.childNodes.length; i++) {
                    if (processNode(tempDiv.childNodes[i])) {
                        break;
                    }
                }

                const finalDiv = document.createElement('div');
                resultNodes.forEach(node => finalDiv.appendChild(node));
                return finalDiv.innerHTML + (needsEllipsis ? '...' : '');
            }

            function getPureTextFromHTML(htmlString) {
                const tempDiv = document.createElement('div');
                tempDiv.innerHTML = htmlString;
                return tempDiv.textContent || tempDiv.innerText;
            }

            const pureTextContent = getPureTextFromHTML(fullContentHTML);

            let needsExpander = false;
            if (limitType === 'words' && pureTextContent.split(/\s+/).filter(w => w.length > 0).length > limitValue) {
                needsExpander = true;
            } else if (limitType === 'characters' && pureTextContent.length > limitValue) {
                needsExpander = true;
            }

            if (!needsExpander) {
                wrapper.classList.add('no-expander-needed');
                intermediateDiv.style.maxHeight = 'none';
                intermediateDiv.style.overflow = 'visible';
                return;
            }
            
            wrapper.appendChild(toggleButton);

            const truncatedContentHTML = truncateContentWithHtml(fullContentHTML, limitType, limitValue);
            
            function measureElementHeight(element, contentToSet = null, includeButton = false) {
                const tempWrapper = wrapper.cloneNode(true);
                tempWrapper.style.cssText = `
                    position: absolute !important;
                    visibility: hidden !important;
                    top: -9999px !important;
                    left: -9999px !important;
                    width: ${wrapper.offsetWidth}px !important;
                    max-height: none !important;
                    overflow: visible !important;
                `;
                
                const tempIntermediateDiv = tempWrapper.querySelector('.uk-card.uk-card-default');
                const tempContentDiv = tempWrapper.querySelector('.my-expander-content');
                const tempButton = tempWrapper.querySelector('.my-expander-toggle');

                if (contentToSet) {
                    tempContentDiv.innerHTML = contentToSet;
                }
                
                if (tempButton) {
                    tempButton.style.display = 'block'; 
                }

                document.body.appendChild(tempWrapper);
                
                let height = 0;
                if (element === intermediateDiv) {
                    height = tempIntermediateDiv.scrollHeight;
                } else if (element === wrapper) {
                    height = tempIntermediateDiv.scrollHeight;
                    if (includeButton && tempButton) {
                        const buttonComputedStyle = window.getComputedStyle(tempButton);
                        const buttonMarginTop = parseFloat(buttonComputedStyle.marginTop);
                        height += tempButton.offsetHeight + buttonMarginTop;
                    }
                }
                
                document.body.removeChild(tempWrapper);
                return height;
            }

            const collapsedCardHeight = measureElementHeight(intermediateDiv, truncatedContentHTML);
            const fullCardHeight = measureElementHeight(intermediateDiv, fullContentHTML);

            const collapsedWrapperHeight = measureElementHeight(wrapper, truncatedContentHTML, true);
            const fullWrapperHeight = measureElementHeight(wrapper, fullContentHTML, true);
            
            wrapper.style.transition = 'max-height 0.5s ease-in-out';
            intermediateDiv.style.transition = 'max-height 0.5s ease-in-out';

            wrapper.style.maxHeight = fullWrapperHeight + 'px';
            intermediateDiv.style.maxHeight = fullCardHeight + 'px';
            
            contentDiv.innerHTML = truncatedContentHTML;
            intermediateDiv.style.setProperty('overflow', 'hidden', 'important'); 

            setTimeout(() => {
                wrapper.classList.add('is-collapsed');
                wrapper.style.setProperty('max-height', collapsedWrapperHeight + 'px', 'important');
                intermediateDiv.style.setProperty('max-height', collapsedCardHeight + 'px', 'important');
                wrapper.style.setProperty('overflow', 'visible', 'important');

            }, 50);

            toggleButton.addEventListener('click', function() {
                if (wrapper.classList.contains('is-collapsed')) {
                    wrapper.classList.remove('is-collapsed');
                    wrapper.classList.add('is-expanded');
                    
                    contentDiv.innerHTML = fullContentHTML;
                    intermediateDiv.style.setProperty('max-height', fullCardHeight + 'px', 'important');
                    intermediateDiv.style.setProperty('overflow', 'visible', 'important'); 

                    wrapper.style.setProperty('max-height', fullWrapperHeight + 'px', 'important');
                    wrapper.style.setProperty('overflow', 'visible', 'important');
                    
                    this.textContent = buttonTexts[currentLanguage].readLess;

                } else {
                    wrapper.classList.remove('is-expanded');
                    wrapper.classList.add('is-collapsed');
                    
                    intermediateDiv.style.setProperty('overflow', 'hidden', 'important'); 

                    wrapper.style.setProperty('max-height', collapsedWrapperHeight + 'px', 'important');
                    intermediateDiv.style.setProperty('max-height', collapsedCardHeight + 'px', 'important');

                    wrapper.addEventListener('transitionend', function handler() {
                        if (wrapper.classList.contains('is-collapsed')) {
                            contentDiv.innerHTML = truncatedContentHTML;
                            toggleButton.textContent = buttonTexts[currentLanguage].readMore;
                        }
                        wrapper.removeEventListener('transitionend', handler);
                    });
                }
            });
        });
    }, 200);
});</script><script src="https://progresja.com/wp-content/themes/yootheme/packages/theme-analytics/app/analytics.min.js?ver=4.5.24" defer></script>
